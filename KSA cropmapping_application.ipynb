    "       'xxxxxxxxxxxxxxxx': '020206',\n",
    "       'xxxxxxxxxxxxxxx': '008000',}\n",
    "                map_layout.add_legend(legend_title=\"NDWI\", legend_dict=legend_dict)\n",
    "                \n",
    "                        #MSAVI(Modified Soil-Adjusted Vegetation Index)\n",
    "                       # compute MSAVI2 using fluent API\n",
    "            elif VI_indices.value == 'Modified Soil-Adjusted Vegetation Index (MSAVI)':\n",
    "                composite= sentinel_2A.divide(10000)\n",
    "                msavi2 = composite.select('B8').multiply(2).add(1) \\\n",
    "           .subtract(composite.select('B8').multiply(2).add(1).pow(2) \\\n",
    "           .subtract(composite.select('B8').subtract(composite.select('B4')).multiply(8)).sqrt()\n",
    "            ).divide(2)\n",
    "                msavi2_vispar={'min':-0.1, 'max':0.5, 'palette': ['yellow', 'brown','green']}#MSAVI visualization parameters\n",
