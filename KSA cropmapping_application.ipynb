    "# Click event handler for selection points from DataBase\n",
    "def submit_clicked(b):\n",
    "    with output_widget:\n",
    "        output_widget.clear_output()\n",
    "        try:\n",
    "            global connection\n",
    "            connection=ps.connect(dbname=\"crop_mapping\",#Database_name.\n",
    "                 user=\"postgres\",#Database_user.\n",
    "                 password=\"",#Database_passcode.\n",
    "                 host=\"",#Db Host\n",
    "                 port=\"5432\"#Db operational port\n",
    "                )\n",
    "            global sql\n",
    "         \n",
    "            sql=\"\"\"select*FROM crop_mapping\n",
    "                \"\"\"\n",
    "            gdf = geemap.read_postgis(sql, connection)#Crop_mapping data based on connection to KSA DB\n",
    "            gdf.crs = {'init': 'epsg:4326'} #Reprojection of training points or polygon into unique reference code i.e 4326.\n",
    "            global feature_collection\n",
    "            feature_collection = geemap.gdf_to_ee(gdf)#FROM GDF TO ee feature collection\n",
    "            style = {'color': '131313', 'fillColor':' FD0505'}\n",
    "            map_layout.addLayer(feature_collection.style(**style),{}, \"Field Points\")#Add feature collection features to the layers.\n",
    "#             map_layout.centerObject(feature_collection, 12)#Center the map view to feature collection\n",
    "            \n",
    "        except Exception as e:\n",
    "            print('Please select Your project....')\n",
    "        else:\n",
    "            print(\"Points Succefully loaded\")\n",
    "        \n",
    "           \n",
    "submit.on_click(submit_clicked)\n",
    "\n"
   ]
