    "    'min': 0,\n",
    "    'max': 1,\n",
    "    'palette':['yellow', 'brown','green'],\n",
    "}\n",
    "                colors = vis_params['palette']\n",
    "                vmin = vis_params['min']\n",
    "                vmax = vis_params['max']\n",
    "                map_layout.add_colorbar(vis_params,label='MSAVI')\n",
    "                \n",
    "                map_layout.addLayerControl()\n",
    "                \n",
    "                \n",
    "                legend_dict = {\n",
    "        'xxxxxxxxxxxxxx': '1D26B3',\n",
    "       'xxxxxxxxxxxxxxxx': 'F51294',\n",
    "       'xxxxxxxxxxxxxxxx': '020206',\n",
    "       'xxxxxxxxxxxxxxx': '008000',}\n",
    "                map_layout.add_legend(legend_title=\"MSAVI\", legend_dict=legend_dict)\n",
    "     #EVI(Enhanced Vegetation Index)\n",
    "            elif VI_indices.value == 'Enhanced Vegetation Index (EVI)':\n",
    "                EVI = sentinel_2A.expression(\n",
